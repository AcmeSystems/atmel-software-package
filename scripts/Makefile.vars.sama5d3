CONFIG_BOARD_SAMA5D3_EK = y
endif

ifeq ("y",$(TARGET:%-generic="y"))
CONFIG_SOC_SAMA5D36 = y
CONFIG_BOARD_SAMA5D3_GENERIC = y
endif

ifeq ("y",$(TARGET:%-xplained="y"))
